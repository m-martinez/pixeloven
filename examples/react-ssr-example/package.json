  "scripts": {
    "cli": "pixeloven",
    "build": "run-p build:app build:story",
    "build:app": "pixeloven webpack build",
    "build:story": "pixeloven story build",
    "prebuild": "pixeloven delete dist",
    "development": "pixeloven webpack start",
    "document": "pixeloven document ts ./src",
    "predocument": "pixeloven delete docs",
    "env:docker": "cp .env.docker .env",
    "env:local": "cp .env.localhost .env",
    "env:production": "cp .env.localhost .env",
    "generate": "pixeloven-generators generate",
    "lint": "run-p lint:ts lint:scss",
    "lint:ts": "pixeloven lint ts ./src/**/*.{ts,tsx}",
    "lint:scss": "pixeloven lint scss ./src/**/*.scss",
    "pretest": "pixeloven delete coverage",
    "pretty": "run-p pretty:ts pretty:scss",
    "pretty:ts": "pixeloven pretty ts ./src/**/*.{ts,tsx}",
    "pretty:scss": "pixeloven pretty scss ./src/**/*.scss",
    "production": "node ./dist/server.js",
    "start": "run-p start:app start:story",
    "start:app": "pixeloven webpack start",
    "start:story": "pixeloven story start",
    "test": "pixeloven test --color --coverage",
    "test:watch": "pixeloven test watch"
  }
