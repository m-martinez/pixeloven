  "devEngines": {
    "node": "8.x || 9.x || 10.x"
  },
  "scripts": {
    "build": "run-p build:app build:story",
    "build:app": "pixeloven-webpack build",
    "build:story": "pixeloven-docs build",
    "prebuild": "pixeloven-cli compile:clean",
    "clean": "pixeloven-cli clean",
    "development": "pixeloven-webpack-dev-server start",
    "document": "pixeloven-cli document:ts src",
    "predocument": "pixeloven-cli document:clean",
    "env:docker": "cp .env.docker .env",
    "env:local": "cp .env.localhost .env",
    "env:production": "cp .env.localhost .env",
    "generate": "pixeloven-generators generate",
    "lint": "run-p lint:ts lint:scss",
    "lint:ts": "pixeloven-cli lint:ts src/**/*.{ts,tsx}",
    "lint:scss": "pixeloven-cli lint:scss src/**/*.{scss}",
    "pretest": "pixeloven-cli test:clean",
    "pretty": "pixeloven-cli pretty src/**/*.{scss,ts,tsx}",
    "pretty:ts": "pixeloven-cli pretty:ts src/**/*.{ts,tsx}",
    "pretty:scss": "pixeloven-cli pretty:scss src/**/*.{scss}",
    "production": "node ./dist/server.js",
    "start": "run-p start:app start:story",
    "start:app": "pixeloven-webpack-dev-server start",
    "start:story": "pixeloven-docs serve",
    "test": "pixeloven-cli test --color --coverage",
    "test:watch": "pixeloven-cli test:watch"
  }
}
